{
  "name": "overwrite",
  "version": "1.0.3",
  "description": "Forget monkey-patching your dependencies. Completely rewrite any file in a third-party package/module on-the-fly!",
  "author": "Luke Horvat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukehorvat/overwrite.git"
  },
  "bugs": {
    "url": "https://github.com/lukehorvat/overwrite/issues"
  },
  "main": "dist",
  "scripts": {
    "build": "rimraf dist && babel lib -d dist",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "copy-dir": "0.0.8",
    "lodash.isfunction": "3.0.8",
    "rimraf": "2.5.2"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-preset-es2015": "6.6.0"
  },
  "keywords": [
    "node",
    "node_modules",
    "module",
    "package",
    "overwrite",
    "override",
    "rewrite",
    "edit",
    "monkey",
    "patch"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
